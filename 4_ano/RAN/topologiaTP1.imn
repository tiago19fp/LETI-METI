link l1 {
    delay 100
    nodes {n3 n1}
    bandwidth 1000000000
}

link l2 {
    delay 100
    nodes {n1 n2}
    bandwidth 1000000000
}

link l3 {
    delay 100
    nodes {n2 n4}
    bandwidth 1000000000
}

link l4 {
    delay 100
    nodes {n4 n3}
    bandwidth 1000000000
}

link l5 {
    width 5
    color Magenta
    delay 50000
    nodes {n5 n3}
    bandwidth 512000
}

link l6 {
    delay 10
    nodes {n5 n6}
    bandwidth 100000000
}

link l7 {
    delay 50
    nodes {n7 n2}
    bandwidth 100000000
}

annotation a1 {
    iconcoords {483.0 136.0 987.0 566.0}
    type oval
    label {}
    labelcolor black
    fontfamily {Arial}
    fontsize {12}
    color #c0ee39
    width 0
    border black
    canvas c1
}

annotation a2 {
    iconcoords {721.0 270.0}
    type text
    label {CORE Network}
    labelcolor black
    fontfamily {Arial}
    fontsize {20}
    effects {bold}
    canvas c1
}

annotation a3 {
    iconcoords {290.0 270.0}
    type text
    label {Access Link - Residential}
    labelcolor #ae008b
    fontfamily {Arial}
    fontsize 12
    effects {bold}
